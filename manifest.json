{
    "name": "ALERT_API",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "service_worker": "background.js"
    },
    "browser_action": {
        "default_popup": "index.html"
    },
    "permissions":[
        "tabs",
        "https://www.google.com/*",
        "https://www.youtube.com/*"
    ]
}